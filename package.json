{
  "name": "moi-app",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@storybook/react-native": "^3.1.9",
    "babel-eslint": "^7.2.3",
    "babel-plugin-styled-components": "^1.1.7",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-native": "^2.0.1",
    "concurrently": "^3.5.0",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-react-native": "^3.0.1",
    "jest-expo": "~19.0.0",
    "jest-serializer-enzyme": "^1.0.0",
    "jest-styled-components": "^4.3.0",
    "pre-commit": "^1.2.2",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "16.0.0-alpha.12",
    "react-native-scripts": "1.0.0",
    "react-test-renderer": "16.0.0-alpha.12",
    "redux-mock-store": "^1.2.3",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-processor-styled-components": "^0.2.1",
    "url": "^0.11.0"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "node node_modules/jest/bin/jest.js",
    "test:watch": "node node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node node_modules/jest/bin/jest.js --coverage",
    "test:snapshots": "node node_modules/jest/bin/jest.js -u",
    "lint:styles": "stylelint './src/**/*.js'",
    "lint": "eslint ./src/**/*.js",
    "postlint": "npm run lint:styles",
    "lint:autofix": "eslint ./src/**/*.js --fix",
    "storybook": "storybook start -p 7007",
    "dev": "concurrently --kill-others -p \"[{name}]\" -n \"STB,RN\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run storybook\" \"react-native-scripts start\""
  },
  "jest": {
    "preset": "jest-expo",
    "snapshotSerializers": [
      "./node_modules/jest-serializer-enzyme"
    ]
  },
  "pre-commit": {
    "run": [
      "lint",
      "test"
    ]
  },
  "dependencies": {
    "axios": "^0.16.2",
    "chroma-js": "^1.3.4",
    "expo": "^19.0.0",
    "intl": "^1.2.5",
    "prop-types": "^15.5.10",
    "react": "16.0.0-alpha.12",
    "react-intl": "^2.3.0",
    "react-native": "^0.46.1",
    "react-native-drawer": "^2.4.0",
    "react-native-router-flux": "3",
    "react-native-side-menu": "^1.0.2",
    "react-native-view-transformer": "^0.0.28",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.1.1"
  }
}
